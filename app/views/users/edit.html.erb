<br>
<%=form_tag url_for(action: 'update'), method: "patch" do %>

      <div class="form-group">
       <%= label_tag 'First_name' %>
        <br>
        <%= text_field_tag 'first_name', current_user.first_name %>
        <br /> <br />
      </div>
      <div class="form-group">

        <%= label_tag 'Last_name' %>
        <br>
        <%= text_area_tag 'last_name', current_user.last_name %>
        <br /> <br />
      </div>
      <div class="form-group">

        <%= submit_tag "Edit Profile" %>
      </div>
      <% end %>
    </div>
